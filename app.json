{
  "expo": {
    "name": "LMS_Student_MobileApp",
    "slug": "LMS_Student_MobileApp",
    "version": "1.0.0",

    "web": {
      "favicon": "./src/assets/icons/app-icon.png"
    },

    "experiments": {
      "tsconfigPaths": true
    },

    "plugins": [],

    "orientation": "portrait",
    "icon": "./src/assets/icons/app-icon.png",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./src/assets/icons/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/app-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
