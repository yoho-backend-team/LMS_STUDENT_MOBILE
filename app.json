{
  "expo": {
    "name": "LMS_Student_MobileApp",
    "slug": "LMS_Student_MobileApp",
    "version": "1.0.0",
    "web": {
      "favicon": "./src/assets/icons/app-icon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-dev-client",
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/icons/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icons/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.samsupatty.lmsstudentmobileapp"
    },
    "extra": {
      "apiBaseUrl": "https://lms-node-backend-v1.onrender.com/api",
      "eas": {
        "projectId": "4be6d739-252b-4efe-b632-65e11652b17a"
      }
    }
  }
}
